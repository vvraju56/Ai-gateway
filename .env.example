# Public API key for client authentication
PUBLIC_API_KEY=your-public-api-key-here

# Comma-separated list of private API keys for rotation
API_KEYS=key1,key2,key3,key4,key5

# External API endpoint to proxy requests to
EXTERNAL_API_URL=https://api.example.com

# Port for the server (default: 3000)
PORT=3000

# Environment (development/production)
NODE_ENV=development